{
  "displayName": "Alert: Cloud Build Failure",
  "combiner": "OR",
  "enabled": true,
  "conditions": [
    {
      "displayName": "Cloud Build Failure Condition",
      "conditionMatchedLog": {
        "filter": "resource.type=\"build\" AND logName:\"cloud-build\" AND textPayload:(\"FAILURE\" OR \"ERROR\")"
      }
    }
  ],
  "notificationChannels": [
    "projects/cloud-palestra-athena/notificationChannels/3974421470222764039"
  ],
  "alertStrategy": {
    "notificationRateLimit": {
      "period": "300s"
    }
  }
}
